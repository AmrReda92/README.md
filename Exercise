#include<iostream>


using namespace std;


bool isprime (int NumForCheck){
  if(NumForCheck<=1)
    return false;
  for(int i=2;i<NumForCheck/2 ;i++){
    if(NumForCheck%i==0){
      return false;
    } 
  }  
  return true;  
}

void checkprime(int number1,int number2){
  for(int i=number1;i<=number2;i++){
    if(isprime(i)){
      cout<<i<<" ";
    }

  }
}
int main() {

 int m,n; 
 cout<<"Enter two numbers to check all prime numbers between them (start with small number): ";
 cin>>n>>m;

}
 checkprime(n,m);


  return 0;
}
